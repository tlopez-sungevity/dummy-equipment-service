# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

GET     /dummy/inverter                             @controllers.TesController.inverter()
GET     /dummy/module                             @controllers.TesController.module()
GET     /dummy/sirenSpecModule                @controllers.TesController.sirenModule()
GET     /dummy/sirenSpecInverter               @controllers.TesController.sirenInverter()
GET     /dummy/module/:id                        @controllers.EquipmentController.getEquipment(id: Int)
GET    /dummy/siren-spec/inverter             @controllers.EquipmentController.dummyGetInverterProductSpec
GET    /dummy/siren-spec/module             @controllers.EquipmentController.dummyGetModuleProductSpec
